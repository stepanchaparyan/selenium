{
  "name": "automation-testing",
  "version": "0.0.1",
  "description": "automation testing with puppeteer-mocha-chai",
  "scripts": {
    "test": "mocha --reporter mochawesome --require babel-core/register test/main.spec.js --timeout 15000000",
    "eslint": "./node_modules/.bin/eslint src/ test/"
